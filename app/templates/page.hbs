{{#if shouldRenderMore}}
  {{to-nav-stack
      layer=0
      item=(component 'routable-components/yapp/more')
      titleBar=(component 'routable-components/yapp/more/title-bar')
  }}
{{/if}}

{{to-nav-stack
    layer=layerIndex
    item=(component
            routeComponent
            model=model
            layerIndex=layerIndex
            drillDownToScheduleItem=(route-action 'drillDownToScheduleItem')
    )
    titleBar=(component titleBarComponent
      model=model
      visitMySchedule=(route-action 'visitMySchedule')
      backToMorePage=(route-action 'backToMorePage')
    )
}}

{{outlet}}
