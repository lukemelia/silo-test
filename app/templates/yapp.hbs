<div id="layer-0-viewport" class="viewport">
  <div class='y-page-base perspective y-background-lightness-{{model.backgroundLightness}} theme-{{model.backgroundLightness}}'>
    <div class='y-page y-page-base {{themeClasses}}  {{unless showingTabBar 'no-tab-bar'}}'>
      <div class="TitleBar">
        {{liquid-outlet "layer-0-title-bar" class="title-bar-outlet"}}
      </div>
      <div class='silo-container layer-0' style="left: 0;">
        <div class="silo-outlet silo-0-outlet">
          MORE
        </div>
        {{liquid-outlet "layer-0-silo-1" class="silo-outlet silo-1-outlet"}}
        {{liquid-outlet "layer-0-silo-2" class="silo-outlet silo-2-outlet"}}
        {{liquid-outlet "layer-0-silo-3" class="silo-outlet silo-3-outlet"}}
        {{liquid-outlet "layer-0-silo-4" class="silo-outlet silo-4-outlet"}}
      </div>
      <div class="TabBar">
        Tab Bar
      </div>
    </div>
  </div>
</div>

<div id="layer-1-viewport" class="viewport">
  <div class='silo-container layer-1' style="left: 0;">
    {{liquid-outlet "layer-1-silo-0" class="silo-outlet silo-0-outlet"}}
    {{liquid-outlet "layer-1-silo-1" class="silo-outlet silo-1-outlet"}}
    {{liquid-outlet "layer-1-silo-2" class="silo-outlet silo-2-outlet"}}
    {{liquid-outlet "layer-1-silo-3" class="silo-outlet silo-3-outlet"}}
  </div>
</div>

<div id="layer-2-viewport" class="viewport">
  <div class='silo-container layer-2' style="left: 0;">
    {{liquid-outlet "layer-2-silo-0" class="silo-outlet silo-0-outlet"}}
  </div>
</div>
