{{to-elsewhere
    named=(concat 'layer-' layerIndex '-silo-' siloIndex)
    send=(component
            routeComponent
            model=model
            layerIndex=layerIndex
            siloIndex=siloIndex
            rootPage=rootPage
            drillDownToScheduleItem=(route-action 'drillDownToScheduleItem')
            backToMorePage=(route-action 'backToMorePage')
         )
}}

{{to-elsewhere
    named=(concat 'layer-' layerIndex '-title-bar')
    send=(component titleBarComponent
      model=model
      siloIndex=siloIndex
    )
}}

{{outlet}}
