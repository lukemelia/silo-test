{{to-elsewhere
    named=(concat 'layer-' layerIndex '-silo-' siloIndex)
    send=(component
            routeComponent
            model=model
            layerIndex=layerIndex
            siloIndex=siloIndex
            back=(route-action 'back')
         )
}}

{{to-elsewhere named=(concat 'layer-' layerIndex '-title-bar')
    send=(component titleBarComponent
      model=model
      siloIndex=siloIndex
      back=(route-action 'back')
    )
}}

{{outlet}}
