{{to-elsewhere
    named=(concat 'layer-' layerIndex '-silo-' siloIndex)
    send=(component
            routeComponent
            model=model
            layerIndex=layerIndex
            siloIndex=siloIndex
            drillDownToRatingForm=(route-action 'drillDownToRatingForm')
            back=(route-action 'back')
            visitMySchedule=(route-action 'visitMySchedule')

         )
}}

{{to-elsewhere named=(concat 'layer-' layerIndex '-title-bar')
    send=(component titleBarComponent
      model=model
      siloIndex=siloIndex
      back=(route-action 'back')
      visitMySchedule=(route-action 'visitMySchedule')
    )
}}

{{outlet}}
